{
  "name": "darksun",
  "version": "1.0.0",
  "description": "Darksun is a space simulation for orbital mechanics, interplanetary travel, and reentry vehicles using Three.js and a physics engine made to run on the web.",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "vercel-build": "vite build",
    "preview": "vite preview",
