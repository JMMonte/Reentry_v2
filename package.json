    "audit:ai:md": "node scripts/ai-audit-engine.js --markdown",
    "audit:ai:fast": "node scripts/ai-audit-engine.js --no-source --no-examples",
    "audit:file": "node scripts/audit-file.js",
    "audit:dir": "node scripts/audit-codebase.js",
    "audit:json": "node scripts/audit-codebase.js --json",
    "audit:html": "node scripts/audit-codebase.js --html",
    "analyze:codebase": "node analyze-codebase.js",
    "analyze:physics": "node analyze-physics.js"
